{
  "name": "landing-page_dinni",
  "version": "1.0.0",
  "description": "Professional landing page for Dinni Rahmawati - Career and Self-Development Practitioner",
  "main": "dist/index.html",
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build": "npm run clean && npm run build:css && npm run build:js && npm run build:html && npm run optimize:images",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify && npm run optimize:css",
    "build:tailwinds": "npm run build:css",
    "build:js": "node scripts/build-js.js",
    "build:html": "node scripts/minify-html.js",
    "optimize:css": "postcss ./dist/output.css --use cssnano --output ./dist/output.css",
    "optimize:images": "node scripts/optimize-images.js",
    "purge:css": "purgecss --css ./dist/output.css --content './dist/**/*.html' './dist/**/*.js' --output ./dist/",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "serve": "npx http-server dist -p 3000 -o -c-1 --gzip",
    "serve:prod": "npx http-server dist -p 3000 -o -c3600 --gzip --brotli",
    "clean": "rimraf dist/output.css dist/main.min.js dist/optimized",
    "prebuild": "npm run clean",
    "postbuild": "npm run analyze:bundle"
  },
  "keywords": [
    "landing-page",
    "tailwindcss",
    "responsive",
    "career-mentoring",
    "professional-website"
  ],
  "author": "Dinni Rahmawati",
  "license": "ISC",
  "dependencies": {
    "flowbite": "^2.5.2",
    "tailwindcss": "^3.4.16"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "esbuild": "^0.19.0",
    "html-minifier-terser": "^7.2.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.0",
    "purgecss": "^5.0.0",
    "rimraf": "^5.0.5",
    "terser": "^5.24.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jokskuyy/Landing-Page-Dinni.git"
  }
}
